
{
    "manifest_version": 2,
    "name": "Bit Protector",
    "version": "1.0",
    "description": "A extension to find dark pattern in your web browser",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "index.html"
    },
    "icons": {
      "48": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  }
  